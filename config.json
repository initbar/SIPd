{
    "project": {
        "description": "active-recording Session Initiation Protocol daemon",
        "repository": "https://github.com/initbar/sipd",
        "branch": "master",
        "version": "1.4.0"
    }
    "sip": {
        "server": {
            "address": null,
            "port": 5060
        },
        "worker": {
            "count": 2
        },
        "headers": {
            "Accept": "application/sdp",
            "Accept-Language": "en",
            "Allow": "ACK, BYE, CANCEL, INVITE, OPTIONS, REFER, UPDATE",
            "Max-Forwards": 70,
            "Min-SE": 1800,
            "Server": "SIPd",
            "Session-Expires": "1800;refresher=uac",
            "Supported": "timer",
            "Require": "timer"
        }
    },
    "sdp": {
        "headers": {
            "v": 0,
            "t": "0 0",
            "c": "IN IP4 %s",
            "s": "phone-call",
            "m": "audio %s RTP/AVP 0 8 18 96",
            "a": [
                "rtpmap:0 PCMU/8000",
                "rtpmap:8 PCMA/8000",
                "rtpmap:18 G729/8000",
                "rtpmap:96 telephone-event/8000",
                "fmtp:96 0-15",
                "recvonly",
                "ptime:20",
                "maxptime:1000"
            ]
        }
    },
    "rtp": {
        "handlers": [
            {
                "enabled": true,
                "host": "127.0.0.1",
                "port": 5900,
                "role": {
                    "active": true,
                    "standby": true
                }
            }, {
                "enabled": false,
                "host": "127.0.0.1",
                "port": 5901,
                "role": {
                    "active": false,
                    "standby": true
                }
            }
        ]
    },
    "gc": {
        "call_lifetime": 3600,
        "loop_interval": 0.01
    },
    "db": {
        "enabled": false,
        "host": "127.0.0.1",
        "port": 3306,
        "username": "",
        "password": ""
    },
    "log": {
        "console": {
            "enabled": true
        },
        "filesystem": {
            "enabled": false,
            "path": "./logs",
            "name": "sipd.log",
            "total_days": 7,
            "gzip": null
        },
        "coloredlogs": false,
        "level": "DEBUG"
    }
}

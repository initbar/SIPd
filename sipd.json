{
    "program": "sipd -- Active recording Session Initiation Protocol daemon.",
    "source": "https://github.com/initbar/sipd",
    "sip": {
        "server": {
            "address": ""
        },
        "router": {
            "port": 5060,
            "nodes": []
        },
        "worker": {
            "count": 2,
            "headers": {
                "Accept": "application/sdp",
                "Accept-Language": "en",
                "Allow": "ACK, BYE, CANCEL, INVITE, OPTIONS, REFER, UPDATE",
                "Max-Forwards": 70,
                "Min-SE": 1800,
                "Server": "SIPd",
                "Session-Expires": "1800;refresher=uac",
                "Supported": "timer",
                "Require": "timer"
            }
        }
    },
    "sdp": {
        "headers": {
            "v": 0,
            "t": "0 0",
            "c": "IN IP4 %s",
            "s": "phone-call",
            "m": "audio %s RTP/AVP 0 8 18 96",
            "a": [
                "rtpmap:0 PCMU/8000",
                "rtpmap:8 PCMA/8000",
                "rtpmap:18 G729/8000",
                "rtpmap:96 telephone-event/8000",
                "fmtp:96 0-15",
                "recvonly",
                "ptime:20",
                "maxptime:1000"
            ]
        }
    },
    "rtp": {
        "max_retry": 1,
        "handler": [
            {
                "enabled": true,
                "host": "127.0.0.1",
                "port": 5900
            }
        ]
    },
    "db": {
        "enabled": false,
        "host": "127.0.0.1",
        "port": 3306,
        "username": "",
        "password": ""
    },
    "gc": {
        "call_lifetime": 3600,
        "check_interval": 0.01
    },
    "log": {
        "console": {
            "enabled": true
        },
        "filesystem": {
            "enabled": false,
            "path": "./sipd",
            "name": "sipd.log",
            "total_days": 7
        },
        "coloredlogs": false,
        "level": "INFO"
    }
}

{
    "program": "sipd -- Active recording Session Initiation Protocol daemon.",
    "source": "https://github.com/initbar/sipd",
    "docs": "https://github.com/initbar/sipd/blob/master/docs/CONFIG.md",
    "sip": {
        "server": {
            "address": "<overridden>"
        },
        "router": {
            "port": 5060,
            "nodes": []
        },
        "worker": {
            "count": 1,
            "headers": {
                "Allow": "ACK, BYE, CANCEL, INVITE, OPTIONS, REFER, UPDATE",
                "Max-Forwards": 70,
                "Require": "timer",
                "Server": "sipd",
                "Session-Expires": 1800,
                "Supported": "timer, uui"
            }
        }
    },
    "sdp": {
        "headers": {
            "v": 0,
            "t": "0 0",
            "c": "IN IP4 %s",
            "s": "phone-call",
            "m": "audio %s RTP/AVP 0 8 18 96",
            "a": [
                "rtpmap:0 PCMU/8000",
                "rtpmap:8 PCMA/8000",
                "rtpmap:18 G729/8000",
                "rtpmap:96 telephone-event/8000",
                "fmtp:96 0-15",
                "recvonly",
                "ptime:20",
                "maxptime:1000"
            ]
        }
    },
    "rtp": {
        "max_retry": 1,
        "handler": [
            {
                "enabled": true,
                "host": "127.0.0.1",
                "port": 5900
            }, {
                "enabled": false,
                "host": "127.0.0.1",
                "port": 5901
            }
        ]
    },
    "database": {
        "impala": {
            "enabled": false,
            "host": "127.0.0.1",
            "port": 21050,
            "username": "",
            "password": ""
        },
        "mssql": {
            "enabled": false,
            "host": "127.0.0.1",
            "port": 1433,
            "username": "",
            "password": ""
        },
        "mysql": {
            "enabled": false,
            "host": "127.0.0.1",
            "port": 3306,
            "username": "",
            "password": "",
            "database": "",
            "db_table": ""
        }
    },
    "gc": {
        "call_lifetime": 3600,
        "check_interval": 0.1
    }
}
